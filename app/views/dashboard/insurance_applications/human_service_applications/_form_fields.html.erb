                <%= f.label "Plug in", class: "control-label" %>
              </div>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.check_box :hard_wired_gps_telematics %>
                <%= f.label "Hard Wired", class: "control-label" %>
              </div>
            </div>

            <div class="col-md-3">
              <div class="form-group">
                <%= f.check_box :mobile_phone_gps_telematics %>
