          </div>

          <div class="row">
            <div class="col-md-10">
              <li><%= f.label automobile[:transport_governmental_or_public_clients], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :transport_governmental_or_public_clients, :true %>
                <%= label_tag :transport_governmental_or_public_clients_yes, "Yes" %>
                <%= f.radio_button :transport_governmental_or_public_clients, :false %>
