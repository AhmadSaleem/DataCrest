          <div class="row">
            <div class="col-md-8">
              <li><%= f.label automobile[:fleet_telematics_percentage], class: "control-label" %></li>
            </div>

            <div class="col-md-4">
              <div class="form-group">
                <%= f.number_field :fleet_telematics_percentage, step: :any, class: "form-control" %>
              </div>
            </div>
          </div>
