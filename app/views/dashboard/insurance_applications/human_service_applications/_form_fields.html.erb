      </div>
    </div>

    <div class="card" id="general-liability">
      <div class="header">
        <h3 class="title">SECTION III – GENERAL LIABILITY</h3>
      </div>
      <hr>

      <div class="content p-2">
        <ol>
          <div class="row">
            <div class="col-md-12">
              <li><%= f.label general_liability[:operation_narrative], class: "control-label" %></li>
            </div>
          </div>

          <div class="row">
            <div class="col-md-12">
              <div class="form-group">
                <%= f.text_field :operation_narrative, class: "form-control" %>
              </div>
            </div>
          </div>

