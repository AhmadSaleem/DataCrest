          </div>

          <div class="row">
            <div class="col-md-6">
              <li><%= f.label life_safety[:central_meeting_point], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :central_meeting_point, :true %>
                <%= label_tag :central_meeting_point_yes, "Yes" %>
                <%= f.radio_button :central_meeting_point, :false %>
                <%= label_tag :central_meeting_point_no, "No" %>
              </div>
            </div>

            <div class="col-md-4">
              <%= f.text_field :meeting_point_locations, class: "form-control" %>
            </div>
          </div>

          <div class="row">
            <div class="col-md-6">
              <li><%= f.label life_safety[:exit_signs], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :exit_signs, :true %>
                <%= label_tag :exit_signs_yes, "Yes" %>
