                <%= f.radio_button :sprinklers, :false %>
                <%= label_tag :sprinklers_no, "No" %>
              </div>
            </div>

            <div class="col-md-4">
              <%= f.text_field :sprinkler_locations, class: "form-control" %>
            </div>
          </div>

          <div class="row">
            <div class="col-md-6">
              <li><%= f.label life_safety[:exterior_evacuation_routes], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :exterior_evacuation_routes, :true %>
                <%= label_tag :exterior_evacuation_routes_yes, "Yes" %>
                <%= f.radio_button :exterior_evacuation_routes, :false %>
