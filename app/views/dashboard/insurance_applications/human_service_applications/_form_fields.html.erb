    </div>

    <div class="card" id="automobile">
      <div class="header">
        <h3 class="title">SECTION II - AUTOMOBILE</h3>
      </div>
      <hr>

      <div class="content p-2">
        <ol>
          <div class="row">
            <div class="col-md-12">
              <li><%= f.label automobile[:vehicle_use] %></li>
            </div>
          </div>

          <div class="row">
            <div class="col-md-6">
              <div class="form-group">
                <%= f.label "Employees Percentage", class: "control-label" %><br>
                <%= f.number_field :percentage_of_employee, step: :any, class: "form-control" %>
              </div>
            </div>

            <div class="col-md-6">
              <div class="form-group">
                <%= f.label "Volunteers Percentage", class: "control-label" %><br>
                <%= f.text_field :percentage_of_volunteer, step: :any, class: "form-control" %>
              </div>
            </div>

            <div class="col-md-12">
              <div class="form-group">
                <%= f.label "Describe use", class: "control-label" %><br>
                <%= f.text_field :vehicle_use, class: "form-control" %>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-10">
              <li><%= f.label automobile[:require_employees_and_volunteers], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :require_employees_and_volunteers, :true %>
                <%= label_tag :require_employees_and_volunteers_yes, "Yes" %>
                <%= f.radio_button :require_employees_and_volunteers, :false %>
                <%= label_tag :require_employees_and_volunteers_no, "No" %>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-4">
              <li><%= f.label automobile[:required_limits], class: "control-label" %></li>
            </div>

            <div class="col-md-8">
              <div class="form-group">
                <%= f.text_field :required_limits, class: "form-control" %>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-10">
              <li><%= f.label automobile[:use_mvr], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :use_mvr, :true %>
                <%= label_tag :use_mvr_yes, "Yes" %>
                <%= f.radio_button :use_mvr, :false %>
                <%= label_tag :use_mvr_no, "No" %>
              </div>
            </div>

            <div class="col-md-12">
              <div class="form-group">
                <%= f.label "if yes, how often?", class: "control-label" %><br>
                <%= f.text_field :how_often_use_mvr, class: "form-control" %>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-10">
              <li><%= f.label automobile[:driver_safety_program], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :driver_safety_program, :true %>
                <%= label_tag :driver_safety_program_yes, "Yes" %>
                <%= f.radio_button :driver_safety_program, :false %>
                <%= label_tag :driver_safety_program_no, "No" %>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-10">
              <li><%= f.label automobile[:transport_clients], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :transport_clients, :true %>
                <%= label_tag :transport_clients_yes, "Yes" %>
                <%= f.radio_button :transport_clients, :false %>
                <%= label_tag :transport_clients_no, "No" %>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-10">
              <li><%= f.label automobile[:new_employee_training], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :new_employee_training, :true %>
                <%= label_tag :new_employee_training_yes, "Yes" %>
                <%= f.radio_button :new_employee_training, :false %>
                <%= label_tag :new_employee_training_no, "No" %>
