              <div class="form-group">
                <%= f.label "If yes, for a fee?", class: "control-label" %><br>
              </div>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :transport_fee, :true %>
                <%= label_tag :transport_fee_yes, "Yes" %>
                <%= f.radio_button :transport_fee, :false %>
                <%= label_tag :transport_fee_no, "No" %>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-md-10">
              <li><%= f.label automobile[:utilize_gps_fleets], class: "control-label" %></li>
            </div>

            <div class="col-md-2">
              <div class="form-group">
                <%= f.radio_button :utilize_gps_fleets, :true %>
                <%= label_tag :utilize_gps_fleets_yes, "Yes" %>
                <%= f.radio_button :utilize_gps_fleets, :false %>
                <%= label_tag :utilize_gps_fleets_no, "No" %>
